Host *-priv
    UserKnownHostsFile=/dev/null
    StrictHostKeyChecking no
    GSSAPIAuthentication no
    GSSAPIDelegateCredentials no
    AddKeysToAgent yes
    IdentityFile ~/.ssh/sbn_rsa
    PasswordAuthentication no
    ForwardAgent yes
    Protocol 2
    AddressFamily inet
    ServerAliveInterval 60
    ForwardX11 yes
    

