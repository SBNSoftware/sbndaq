   DACOffset2: [ 0xF000, 0xD000, 0xC000, 0xF010, 0xD010, 0xC010, 0xF100, 0xD100, 0xC100 ]
   
   separate_data_thread: true

   destinations:{ }

   routing_table_config: {
     use_routing_master: false 
  }
}  		 

