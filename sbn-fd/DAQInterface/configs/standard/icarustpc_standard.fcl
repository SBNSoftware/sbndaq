   CompressionScheme: 0

   VetoPort: 7654
   VetoHost: "137.138.194.204"
   DoVetoTest: false
   VetoTestPeriod: 1e6

   pcieLinks: [ 0, 1 ]

   TestPulseType: 0 #0 - disable, 1 - external, 2 - internal_even, 3 - internal_odd
